<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home</title>
</head>
<body>
    <% Quizes !== undefined || Quizes.length != 0  ? Quizes.map( quiz => { %>
        <a href="quiz/s/<%= quiz._id %>">
            <h3><%= quiz.name %> - <small><small><%= quiz.author %></small></small></h3>
        </a>
    <% }): "" %>
</body>
</html>